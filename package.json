    "@types/request-promise-native": "^1.0.15",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "^2.7.0",
    "jest": "^22.4.3",
    "release-it": "^7.5.0",
    "ts-jest": "^23.0.0",
    "tslint": "^5.10.0",
    "tslint-eslint-rules": "^5.3.1",
    "typedoc": "^0.11.1",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "demux": "^1.0.3",
    "request": "^2.87.0",
